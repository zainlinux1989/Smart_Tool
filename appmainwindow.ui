   <string notr="true">/* Main Window style setting */
QMainWindow
{	
			/* white to grey linear gradient */
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 rgba(251, 251, 251, 255), stop:1 rgba(241, 241, 241, 255));
}

/* Tab Widget style setting */
QTabWidget::pane		
{	
	background-color: transparent; 

	border-color: rgb(77, 208, 225);		/* sky blue color */	
	border-radius:5px;
	border-width:2px;
	/*border-style:solid;*/
}

/* Selected Tab Bar style setting */
QTabBar::tab:selected
{
	
	background-color: transparent; 

	border-color: rgb(77, 208, 225);		/* sky blue color */	
	color:rgb(77, 208, 225);		/* sky blue color */
	border-radius:5px;
	border-width:2px;
	border-style:solid;
	
	margin-left: 5px;
	margin-right: 5px;
	padding: 3px 5px;
}

/* Not selected Tab Bar style setting */
QTabBar::tab:!selected
{	
	background-color: transparent; 
		
	border-color: rgb(224, 224, 224);		/* grey color */
	color:rgb(224, 224, 224);		/* grey color */
	border-radius:5px;
	border-width:1px;
	border-style:solid;

	margin-left: 5px;
	margin-right: 5px;
	padding: 3px 5px;
}

/* QLabels style setting */
QLabel
{
	font-family: Cantarell;
	font-size:12px;
}

/* QLineEdit style setting */
QLineEdit
{
	min-height:22px;
	max-height:22px;	
	font-family: Cantarell;
	font-size:12px;
	background-color: rgb(224, 224, 224);		/* grey color */
	border-color: rgb(77, 208, 225);		/* sky blue color */
	border-width:2px;
	border-style:solid;
}

/* QCheckBox  style setting*/
QCheckBox
{
	min-height:22px;
	max-height:22px;	
	font-family: Cantarell;
	font-size:12px;
}

/* QCheckBox Indicator  style setting */
QCheckBox::indicator
{
	min-height:20px;
	max-height:20px;	
	min-width:20px;
	max-width:20px;	

